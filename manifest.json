{
    "manifest_version": 3,
    "name": "GistFlow",
    "version": "1.0",
    "description": "A page summarisation tool using chrome built in AI",
    "icons": {
      "16": "public/images/icon.jpg"
    },
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
    "storage",
    "tabs",
    "scripting",
    "activeTab"
    ],
    "background": {
      "service_worker": "dist/background.js"
    }
  }
  
